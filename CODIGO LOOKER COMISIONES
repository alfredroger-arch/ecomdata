#COMENTARIOS

CASE WHEN OBS IS NULL THEN CONCAT(Coment2," / ","Comisión real sin penalizaciones: ",TOTAL COMIS.," / "," Tiene ",Anuladas," anulaciones (",MONTO_VENTA*Anuladas,")") ELSE
CONCAT(Coment2," / ","Comisión real sin penalizaciones: ",TOTAL COMIS.," / ",OBS," / "," Tiene ",Anuladas," anulaciones (",PAGO VENTA *Anuladas,")")
END 

#MONTO COMISION
CASE WHEN CAMPAÑA != "COMPRA PROTEGIDA" THEN COALESCE(FUNO ADULTO,FUNO DENTAL,FUNO RENTA H.,CFS ADULTO,CFS DENTAL,BANBIF RENTA H.,BANBIF DENTAL,COMPRA PROTEGIDA,
         CHUBB - RIPLEY ONCOLOGICO,RIPLEY  - ACCIDENTES PERSONALES,RIPLEY DENTAL,RIPLEY RETENCIONES,INTERSEGUROS VEHICULAR,
         INTERSEGUROS VIDA Y RUMBO,INTERSEGUROS VIAJES,INTERSEGUROS SOAT,CLINICA RICARDO PALMA,AUNA ONCOSALUD,AUNA ONCOFLEX
	    )
